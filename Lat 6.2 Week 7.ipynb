        "import math\n",
        "n = int(input(\"Masukkan n\\n>> \"))\n",
        "for i in range (n,0,-1):\n",
        "    print(math.factorial(i), end=\" \")\n",
        "    for j in range(i, 0, -1):\n",
        "        print(j, end=\" \")\n",
        "    print()"
